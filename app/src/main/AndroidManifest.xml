<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.somanshu.paperround">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SportActivity"
            android:label="News" />
        <activity
            android:name=".EntActivity"
            android:label="Entertainment News" />
        <activity
            android:name=".BusinessActivity"
            android:label="Business News" />
        <activity
            android:name=".TechActivity"
            android:label="Technology News" />
        <activity
            android:name=".NatureActivity"
            android:label="Nature News" />
        <activity android:name=".NewsDetails" />
        <activity android:name=".NewsDetailsActivity" />
        <activity android:name=".NewsActivity"></activity>
    </application>

</manifest>